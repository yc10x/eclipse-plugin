	<!-- Package Explorer: Classfile level -->
	<extension point="org.eclipse.ui.popupMenus">
		<objectContribution
				objectClass="org.eclipse.jdt.core.ICompilationUnit"
				nameFilter="*.java"
				id="de.jayefem.log4e.RootContribution">
			<menu label="%logger.menu.label" path="additions"
				id="de.jayefem.log4e.LoggerMenu">
				<separator name="declareGroup"/>
				<groupMarker name="addGroup"/>
				<groupMarker name="errorLogGroup"/>
				<groupMarker name="modifyGroup"/>
				<groupMarker name="removeGroup"/>
				<groupMarker name="exchangeGroup"/>
				<groupMarker name="replaceGroup"/>
			</menu>
			<!-- Declare -->
			<action
			label="%logger.declare.label"
				tooltip="%logger.declare.tooltip"
				class="de.jayefem.log4e.popup.actions.ResourcePopupAction"
				menubarPath="de.jayefem.log4e.LoggerMenu/declareGroup"
				enablesFor="*"
				id="de.jayefem.log4e.DeclareAction">
			</action>
			<!-- Insert -->
			<action
				label="%logger.add.label"
				tooltip="%logger.add.tooltip"
				class="de.jayefem.log4e.popup.actions.ResourcePopupAction"
				menubarPath="de.jayefem.log4e.LoggerMenu/addGroup"
				enablesFor="*"
