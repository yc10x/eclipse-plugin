				label="%logger.modify.label"
				tooltip="%logger.modify.tooltip"
				class="de.jayefem.log4e.popup.actions.ResourcePopupAction"
				menubarPath="de.jayefem.log4e.LoggerMenu/modifyGroup"
				enablesFor="*"
